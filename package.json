{
  "name": "dynamic-clock-vue-component",
  "version": "1.0.0",
  "description": "A customizable, animated clock component for Vue 3 with TypeScript support",
  "type": "module",
  "main": "./dist/dynamic-clock.umd.js",
  "module": "./dist/dynamic-clock.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/dynamic-clock.es.js",
      "require": "./dist/dynamic-clock.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc && vite build",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "clock",
    "component",
    "analog",
    "digital",
    "tailwind"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/dynamic-clock-vue-component"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  }
}
